<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LauFitness"
        tools:ignore="ExtraText"
        tools:targetApi="31">
        <activity
            android:name=".PaisActivity"
            android:exported="false"
            android:label="@string/title_activity_pais"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".PaisInfo"
            android:exported="false"
            android:label="@string/title_activity_pais_info"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".InsertarPaisActivity"
            android:exported="false"
            android:label="@string/title_activity_insertar_pais"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".UserDetalleActivity"
            android:exported="false"
            android:label="@string/title_activity_user_detalle"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".UsuarioActivity"
            android:exported="false"
            android:label="@string/title_activity_usuario"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".ClasesActivity"
            android:exported="false"
            android:label="@string/title_activity_clases"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".CategoriasActivity"
            android:exported="false"
            android:label="@string/title_activity_categorias"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".WelcomeActivity"
            android:exported="false"
            android:label="@string/title_activity_welcome"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false"
            android:label="@string/title_activity_register"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".ComunidadActivity"
            android:exported="false"
            android:label="@string/title_activity_comunidad"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".FunctionalActivity"
            android:exported="false"
            android:label="@string/title_activity_functional"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".NutricionActivity"
            android:exported="false"
            android:label="@string/title_activity_nutricion"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".TrainerActivity"
            android:exported="false"
            android:label="@string/title_activity_trainer"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:label="@string/title_activity_splash"
            android:theme="@style/Theme.LauFitness">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:exported="false"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.LauFitness" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.LauFitness" />
    </application>

</manifest>